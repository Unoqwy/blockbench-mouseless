{
    "name": "swathe",
    "description": "Collection of Blockbench plugins",
    "repository": "https://github.com/Unoqwy/swathe",
    "private": true,
    "scripts": {
        "tsc-check": "tsc --noEmit --skipLibCheck",
        "lint": "eslint . --ext js,ts",
        "style": "yarn prettier . -w && yarn lint",
        "bundle": "rm -rf ./dist && spack && rm ./dist/*.map && node ./scripts/post-bundle.js",
        "dev": "yarn tsc-check && yarn bundle"
    },
    "dependencies": {},
    "devDependencies": {
        "@swc/cli": "^0.1.46",
        "@swc/core": "^1.2.60",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "blockbench-types": "https://github.com/Unoqwy/blockbench-types",
        "eslint": "^7.28.0",
        "prettier": "^2.3.1",
        "pretty-ms": "^7.0.1",
        "sass": "^1.35.0",
        "typescript": "^4.3.2",
        "uglify-js": "^3.13.9"
    }
}
